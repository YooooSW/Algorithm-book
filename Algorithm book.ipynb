   "source": [
    "# 문제01 배열 정렬하기\n",
    "import time\n",
    "def solution(arr): # 버블 정렬로 정렬\n",
    "    n = len(arr)\n",
    "    for i in range(n):\n",
    "        for j in range(n - i - 1):\n",
    "            if arr[j] > arr[j + 1]:\n",
    "                arr[j], arr[j + 1] = arr[j + 1], arr[j]\n",
    "    return arr"
   ]
  },
